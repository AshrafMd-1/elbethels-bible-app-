{
  "expo": {
    "name": "gabrial",
    "slug": "gabrial",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/icon-192.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/icon-192.png",
        "backgroundColor": "#121212"
      },
      "package": "com.android.elbthel"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icons/icon-192.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/icon-192.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#121212"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e093658e-a8f7-4a6e-ae32-1a14c2f29ba9"
      }
    }
  }
}
